    "classnames": "^2.5.1",
    "typed.js": "^3.0.0"
  },
  "peerDependencies": {
    "@plitzi/plitzi-sdk": "^0.28",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@plitzi/plitzi-sdk": "^0.28.3",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/react": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@vitejs/plugin-react": "^5.1.4",
    "cssnano": "^7.1.2",
    "eslint": "9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-storybook": "^10.2.8",
    "flow-bin": "^0.301.0",
    "globals": "^17.3.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.97.3",
    "storybook": "^10.2.8",
    "stylelint": "^17.3.0",
    "stylelint-order": "^7.0.1",
    "stylelint-scss": "^7.0.0",
    "tailwindcss": "4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.2.0",
    "vite-plugin-zip-file": "^5.0.0",
    "vitest": "^4.0.18",
    "vitest-fetch-mock": "^0.4.5"
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "dev": "vite preview --port 5000",
    "build:dev": "tsc -p ./tsconfig.app.json && vite build --mode development",
    "build:dev-gzip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true vite build --mode development",
    "build:dev-zip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true AS_ZIP=true vite build --mode development",
    "build:dev-watch": "tsc -p ./tsconfig.app.json && vite build --mode development --watch",
    "build:dev-analyze": "tsc -p tsconfig.app.json --noEmit && ONLY_ANALYZE=true vite build --mode development",
    "build:prod": "tsc -p ./tsconfig.app.json && vite build",
    "build:prod-gzip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true vite build",
    "build:prod-zip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true AS_ZIP=true vite build",
    "build:prod-analyze": "tsc -p tsconfig.app.json --noEmit && ONLY_ANALYZE=true vite build",
    "typecheck": "tsc -p tsconfig.app.json --noEmit",
    "lint": "TIMING=1 eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint-ts": "tsc",
    "lint-style": "stylelint \"**/*.scss\" --custom-syntax postcss-scss",
    "lint-style:fix": "stylelint \"**/*.scss\" --custom-syntax postcss-scss --fix",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.2.2"
}
